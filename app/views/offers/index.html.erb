<h4>NAVBAR</h4>
<div class="container">
  <div class="container-offer-cards">
    <% @offers.each do |offer| %>
      <div class="offer-card">
        <p>Title: <%= offer.title %></p>
        <p>Category: <%= offer.category %></p>
        <p>Price: <%= offer.price %> EUR</p>
        <p>City: <%= offer.city %></p>
      </div>
    <% end %>
    <br>
  </div>
</div>
<p><%= link_to "HOME", root_path %></p>
<h4>FOOTER</h4>

